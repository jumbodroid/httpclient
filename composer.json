{
  "name": "rayalois22/httpclient",
  "description": "HttpClient is an implementation of HTTP Client for PHP and Laravel",
  "license": "MIT",
  "keywords": [
    "HTTP",
    "Client",
    "PHP",
    "Laravel"
  ],
  "authors": [
    {
      "name": "Alois Odhiambo",
      "email": "rayalois22@gmail.com",
      "homepage": "https://github.com/rayalois22",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2.5",
    "illuminate/config": "7.11.*",
    "psr/http-message": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-client": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "8.5.*",
    "mockery/mockery": "^1.3.1",
    "symfony/var-dumper": "^5.0.8"
  },
  "autoload": {
    "psr-4": {
      "Rayalois22\\HttpClient\\": "src",
      "Rayalois22\\Library\\": "src/Library"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rayalois22\\HttpClient\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "stable"
}
