{
  "name": "jumbodroid/httpclient",
  "description": "HttpClient is an implementation of HTTP Client for PHP and Laravel",
  "license": "MIT",
  "keywords": [
    "HTTP",
    "Client",
    "PHP",
    "Laravel"
  ],
  "authors": [
    {
      "name": "Alois Odhiambo",
      "email": "rayalois22@gmail.com",
      "homepage": "https://github.com/rayalois22",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.2.5",
    "ext-curl": "*",
    "psr/http-message": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-client": "^1.0",
    "symfony/options-resolver": ">=5.1"
  },
  "require-dev": {
    "phpunit/phpunit": ">=8.5",
    "mockery/mockery": ">=1.3.1",
    "symfony/var-dumper": ">=5.2.0"
  },
  "provide": {
      "psr/http-client-implementation": "1.0",
      "psr/http-factory-implementation": "1.0",
      "psr/http-message-implementation": "1.0"
  },
  "autoload": {
    "psr-4": {
      "Jumbodroid\\HttpClient\\": "src"
    },
    "files": ["src/Psr7/funclude.php"]
  },
  "autoload-dev": {
    "psr-4": {
      "Jumbodroid\\HttpClient\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "stable"
}
